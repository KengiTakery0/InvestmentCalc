@page
@model InvestmentCalc.Pages.Log_InModel
@{
    <div class="container">
        <h1>Авторизация</h1>
        <form id="login-form">
            <input type="text" id="username" placeholder="Имя пользователя" required>
            <input type="password" id="password" placeholder="Пароль" required>
            <button type="submit">Войти</button>
        </form>
    </div>
}
